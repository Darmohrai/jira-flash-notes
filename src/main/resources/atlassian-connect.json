{
  "key": "ua.cross.kurin.jira-flash-notes",
  "baseUrl": "https://jira-flash-notes.onrender.com",
  "name": "Jira Flash Summary & PDF Generator",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": [
    "READ"
  ],
  "modules": {
    "generalPages": [
      {
        "key": "flash-notes-page",
        "location": "system.top.navigation.bar.apps",
        "name": {
          "value": "Flash Notes PDF"
        },
        "url": "/first-deploy-message"
      }
    ],
    "webItems": [
      {
        "key": "export-pdf-advanced",
        "location": "view.issue.opsbar",
        "weight": 10,
        "name": {
          "value": "Export to PDF"
        },
        "target": {
          "type": "dialog",
          "options": {
            "width": "500px",
            "height": "350px",
            "header": { "value": "Export Settings" }
          }
        },
        "url": "/export-config-dialog?issueKey={issue.key}",
        "context": "addon"
      }
    ]
  }
}